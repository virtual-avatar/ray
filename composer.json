{
    "name": "virtual-avatar/ray",
    "description": "Debug with Ray to fix problems faster",
    "keywords": [
        "spatie",
        "ray"
    ],
    "homepage": "https://github.com/spatie/ray",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/virtual-avatar/backtrace"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ramsey/uuid": "^3.0|^4.1",
        "spatie/macroable": "^1.0|^2.0",
        "virtual-avatar/backtrace": "^1.0",
        "symfony/stopwatch": "^4.0|^5.1|^6.0",
        "symfony/var-dumper": "^4.2|^5.1|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Ray\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Ray\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "phpstan": "vendor/bin/phpstan analyse"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/spatie"
        },
        {
            "type": "other",
            "url": "https://spatie.be/open-source/support-us"
        }
    ]
}
